<% if @from == "unsubscribe" %>
    jQuery("#subscribe_link_pane").html("<%= escape_javascript(render(:partial => "users/subscribe_link", :locals => { :visited_user => @visited_user }))%>");
<% else %>
    jQuery("#subscribe_link_pane").html("<%= escape_javascript(render(:partial => "users/unsubscribe_link", :locals => { :visited_user => @visited_user }))%>");
<% end %>